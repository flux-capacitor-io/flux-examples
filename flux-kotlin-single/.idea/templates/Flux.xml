<templateSet group="Flux">
  <template name="fluxid" value="data class $IdName$(&#10;    val $idName$: String,&#10;) : Id&lt;$AggregateClass$&gt;($idName$)&#10;" description="Flux Aggregate ID Class" toReformat="false" toShortenFQNames="true">
    <variable name="IdName" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="idName" expression="camelCase(IdName)" defaultValue="" alwaysStopAt="true" />
    <variable name="AggregateClass" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="KOTLIN_CLASS" value="true" />
      <option name="KOTLIN_TOPLEVEL" value="true" />
    </context>
  </template>
</templateSet>